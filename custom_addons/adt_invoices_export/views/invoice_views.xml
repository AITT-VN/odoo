<odoo>
    <record id="view_invoice_tree_inherit_export_button" model="ir.ui.view">
        <field name="name">account.invoice.tree.export.custom.button</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_invoice_tree"/>
        <field name="arch" type="xml">
            <header>
                <button name="export_custom_out_invoice" type="object" string="Misa Export" class="oe_highlight"
                    invisible="context.get('default_move_type') not in ('out_invoice')"/>
            </header>
        </field>
    </record>
</odoo>
